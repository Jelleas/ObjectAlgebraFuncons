scope(accum(cl_base,
cl_library),
  accum(seq(print(catch_else_rethrow(
    apply(apply(instantiate_if_poly(follow_if_fwd(bound_value(id('prefix =')))),
      prefer_over(close(patt_abs(any,
        1)),
        abs(throw(cl_match_failure)))),
      prefer_over(close(patt_abs(any,
        2)),
        abs(throw(cl_match_failure)))),
    restrict_domain(prefer_over(close(patt_abs(abs(variant_match('Match_failure',
