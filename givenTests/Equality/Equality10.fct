          given),
        only(tuple_empty))),
        false)),
        prefer_over(close(patt_abs(abs(variant_match('Invalid_argument',
          apply(instantiate_if_poly(bound_value(nameid('variant_selector', id('Invalid_argument')))),
            given),
          any)),
          true)),
          close(patt_abs(any,
            false))))),
      bound_type(nameid('builtin', id('exn')))))),
